{
    "name": "loadboard",
    "version": "1.0.0",
    "description": "Load Board",
    "main": "server/server.js",
    "engines": {
        "node": "20.11.1",
        "npm": "10.5.0"
    },
    "devDependencies": {
        "concurrently": "^5.3.0",
        "nodemon": "^2.0.22"
    },
    "scripts": {
        "server": "nodemon server/server.js",
        "start": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
        "install": "cd server && npm i && cd ../client && npm i",
        "seed": "cd server && npm run seed",
        "build": "cd client && npm run build",
        "predeploy": "cd client && npm run predeploy",
        "deploy": "cd client && npm run deploy"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/larigens/getloadsnow.git"
    },
    "keywords": [
        "react",
        "server",
        "nodejs",
        "expressjs"
    ],
    "author": "Larissa Guilherme",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/larigens/getloadsnow/issues"
    },
    "dependencies": {
        "concurrently": "^5.1.0",
        "gh-pages": "^6.1.1",
        "nodemon": "^2.0.22"
    }
}